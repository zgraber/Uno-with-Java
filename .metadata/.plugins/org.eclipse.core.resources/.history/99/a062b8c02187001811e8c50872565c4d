package cards;
import java.awt.Color;

public class UNOCard {
	private String name;
	private Color color;
	
	public UNOCard(Color color, String name) {
		this.color = color;
		this.name = name;
	}
	
	public String getName() {
		return this.name;
	}
	
	public Color getColor() {
		return this.color;
	}
}
