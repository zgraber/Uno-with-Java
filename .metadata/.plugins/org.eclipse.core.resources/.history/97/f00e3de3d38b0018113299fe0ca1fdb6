import uno.game.Game;
import uno.resources.Constants;

public class Main {

	public static void main(String[] args) {
		boolean newGame = false;
		do {
			Game game = new Game();
			game.startGame();
			System.out.print("New Game(Y/N)?: ");
			
		}while (newGame);
			
		Constants.SCAN.close();
	}

}
